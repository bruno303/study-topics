(cloudflare) {
  tls {
    dns cloudflare {env.CLOUDFLARE_API_TOKEN}
    resolvers 1.1.1.1
  }
}

git.internal.bsoapp.net {
    reverse_proxy gitea:3000
    import cloudflare
}

drive.internal.bsoapp.net {
    reverse_proxy mydrive:3001
    import cloudflare
}
